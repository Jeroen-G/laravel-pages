{
    "name": "jeroen-g/laravel-pages",
    "description": "Simple pages package for Laravel 4.1",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "Jeroen G",
            "email": "jeroeng.jeroeng@gmail.com"
        }
    ],
    "keywords": ["Laravel", "Pages"],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cocur/slugify": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "orchestra/testbench": "2.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "JeroenG\\LaravelPages\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
